        <label>raw copacetic honey</label>
        <description>Raw honey from a copacetic cone. A marvelous raw ingredient!</description>
        <graphicData>
            <texPath>AlienBiomes/Things/Items/Resources/PlantFoodRaw/Honey/SZ_RawCopaceticHoney</texPath>
        </graphicData>
        <statBases>
            <MarketValue>1.3</MarketValue>
            <Mass>0.027</Mass>
        </statBases>
        <comps>
            <li Class="CompProperties_Rottable">
                <daysToRotStart>30</daysToRotStart>
                <rotDestroys>true</rotDestroys>
            </li>
        </comps>
        <ingestible>
            <preferability>RawTasty</preferability>
            <tasteThought>SZ_AteCopaceticHoney</tasteThought>
            <foodType>VegetableOrFruit</foodType>
        </ingestible>
    </ThingDef>

</Defs>
