<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--==================== Radiant Grass ====================-->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_RadiantGrass</defName>
        <label>radiant grass</label>
        <description>A lush and beautiful grass found all over the radiant plains.</description>
        <hideAtSnowDepth>0.1</hideAtSnowDepth>
        <altitudeLayer>LowPlant</altitudeLayer>
        <ingestible />
        <statBases>
            <MaxHitPoints>85</MaxHitPoints>
            <Flammability>1.3</Flammability>
            <Nutrition>0.09</Nutrition>
        </statBases>
        <graphicData>
            <texPath>AlienBiomes/Things/Plants/RadiantGrass</texPath>
            <graphicClass>Graphic_Random</graphicClass>
            <shaderType>CutoutPlant</shaderType>
        </graphicData>
        <plant>
            <fertilityMin>0.05</fertilityMin>
            <fertilitySensitivity>0.3</fertilitySensitivity>
            <growDays>2.5</growDays>
            <leaflessGraphicPath>AlienBiomes/Things/Plants/RadiantGrass_Leafless</leaflessGraphicPath>
            <harvestWork>40</harvestWork>
            <maxMeshCount>9</maxMeshCount>
            <visualSizeRange>0.4~0.6</visualSizeRange>
            <topWindExposure>0.4</topWindExposure>
            <wildOrder>1</wildOrder>
        </plant>
        <modExtensions>
            <li Class="AlienBiomes.BiomePlantControl">
                <terrainTags>
                    <li>Soil</li>
                    <li>Stony</li>
                    <li>Rocky</li>
                    <li>ChaliceSoil</li>
                </terrainTags>
            </li>
        </modExtensions>
    </ThingDef>

