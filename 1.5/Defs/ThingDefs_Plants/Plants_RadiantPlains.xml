    <!--==================== Chalice Fungus ====================-->
    <!-- Only spawns on chalice soil. -->
    <!-- Reacts to pawn movement. -->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_ChaliceFungus</defName>
        <label>chalice fungus</label>
        <description>A tall, chalice-like shaped fungus that grows in patches all throughout the radiant plains. Chalice fungus is incredibly sensitive and reacts to touch.</description>
        <thingClass>AlienBiomes.Plant_Nastic</thingClass>
        <tickerType>Normal</tickerType>
        <drawerType>RealtimeOnly</drawerType>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <hideAtSnowDepth>0.25</hideAtSnowDepth>
        <altitudeLayer>BuildingOnTop</altitudeLayer>
        <ingestible />
        <statBases>
            <MaxHitPoints>46</MaxHitPoints>
            <Flammability>1.00</Flammability>
            <Nutrition>0.02</Nutrition>
        </statBases>
        <graphicData>
            <texPath>AlienBiomes/Things/Plants/ChaliceFungus</texPath>
			<graphicClass>Graphic_Random</graphicClass>
            <shaderType>Transparent</shaderType>
        </graphicData>
        <plant>
            <fertilityMin>0.025</fertilityMin>
            <fertilitySensitivity>0.3</fertilitySensitivity>
            <growDays>22</growDays>
            <leaflessGraphicPath>AlienBiomes/Things/Plants/ChaliceFungus_Leafless</leaflessGraphicPath>
            <visualSizeRange>0.5~2.25</visualSizeRange>
            <topWindExposure>0.35</topWindExposure>
            <wildClusterRadius>4</wildClusterRadius>
            <wildClusterWeight>10</wildClusterWeight>
            <wildOrder>2</wildOrder>
        </plant>
        <modExtensions>
            <li Class="AlienBiomes.BiomePlantControl">
                <terrainTags>
                    <li>ChaliceSoil</li>
                </terrainTags>
            </li>
            <li Class="AlienBiomes.PlantNastic_ModExtension">
                <isVisuallyReactive>true</isVisuallyReactive>
                <isTouchSensitive>true</isTouchSensitive>
                <minScale>0.25</minScale>
                <scaleDeltaDecrease>0.08</scaleDeltaDecrease>
                <scaleDeltaIncrease>0.01</scaleDeltaIncrease>
                <texInstances>3</texInstances>
                <effectRadius>3</effectRadius>
            </li>
        </modExtensions>
    </ThingDef>

    <!--==================== Soothing Stalk ====================-->
    <!-- Only spawns on soothing sand. -->
    <!-- Produces raw soothing honey. -->
    <!-- Harvestable only at night. -->
    <!-- Glows at night. -->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_SoothingStalk</defName>
        <label>soothing stalk</label>
        <description>A vibrant plant only found on the pink sands of radiant plains. They can be farmed for their sweet, pinkish honey. They seemingly spring to life at sundown.</description>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <altitudeLayer>Building</altitudeLayer>
        <fillPercent>0.20</fillPercent>
        <pathCost>30</pathCost>
        <statBases>
            <MaxHitPoints>120</MaxHitPoints>
            <Nutrition>0.25</Nutrition>
            <DeteriorationRate>0.00</DeteriorationRate>
        </statBases>
        <graphicData>
            <texPath>AlienBiomes/Things/Plants/SoothingStalk</texPath>
            <graphicClass>Graphic_Random</graphicClass>
            <shaderType>TransparentPlant</shaderType>
        </graphicData>
        <plant>
            <fertilityMin>0.1</fertilityMin>
            <fertilitySensitivity>0.5</fertilitySensitivity>
            <growDays>16</growDays>
            <leaflessGraphicPath>AlienBiomes/Things/Plants/SoothingStalk_Leafless</leaflessGraphicPath>
            <immatureGraphicPath>AlienBiomes/Things/Plants/SoothingStalk_Immature</immatureGraphicPath>
            <harvestYield>10</harvestYield>
            <harvestWork>250</harvestWork>
            <harvestTag>Standard</harvestTag>
            <harvestAfterGrowth>0.30</harvestAfterGrowth>
            <harvestedThingDef>SZ_RawSoothingHoney</harvestedThingDef>
            <wildClusterRadius>5</wildClusterRadius>
            <wildClusterWeight>30</wildClusterWeight>
            <visualSizeRange>0.95~1.7</visualSizeRange>
            <topWindExposure>0.3</topWindExposure>
            <wildOrder>2</wildOrder>
            <purpose>Food</purpose>
        </plant>
        <comps>
            <li Class="AlienBiomes.CompProperties_TimedGlower">
                <overlightRadius>2.25</overlightRadius>
                <glowRadius>3.25</glowRadius>
                <glowColor>(250, 180, 188, 5)</glowColor>
                <startTime>0.80</startTime>
                <stopTime>0.20</stopTime>
            </li>
            <li Class="AlienBiomes.CompProperties_TimedHarvest">
                <harvestStartTime>0.80</harvestStartTime>
                <harvestStopTime>0.30</harvestStopTime>
                <harvestSeasons>
                    <li>Spring</li>
                    <li>Summer</li>
                </harvestSeasons>
            </li>
        </comps>
        <modExtensions>
            <li Class="AlienBiomes.BiomePlantControl">
                <terrainTags>
                    <li>Sandy</li>
                </terrainTags>
            </li>
        </modExtensions>
    </ThingDef>

