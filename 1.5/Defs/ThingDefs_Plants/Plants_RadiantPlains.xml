    <!--==================== Radiant Bioluminescence ====================-->
    <!-- Only spawns in shallow salty radiant water. -->
    <!-- Fades in and out (alpha channel) at sunset/sunrise. -->

    <ThingDef ParentName="PlantBaseNonEdible">
        <defName>SZ_RadiantBioluminescence</defName>
        <thingClass>AlienBiomes.Plant_Bioluminescence</thingClass>
        <drawerType>MapMeshOnly</drawerType>
        <tickerType>Long</tickerType>
        <label>radiant bioluminescence</label>
        <description>A pretty, bright, and vibrant algae found along the shorelines of radiant plains.</description>
        <selectable>false</selectable>
        <altitudeLayer>LowPlant</altitudeLayer>
        <fillPercent>0.01</fillPercent>
        <pathCost>0.01</pathCost>
        <statBases>
            <MaxHitPoints>1</MaxHitPoints>
            <DeteriorationRate>0.00</DeteriorationRate>
        </statBases>
        <graphicData>
            <texPath>AlienBiomes/Things/Special/ShallowOceanWaterBioluminescence</texPath>
            <graphicClass>Graphic_Random</graphicClass>
            <shaderType>MoteGlowDistorted</shaderType>
            <shaderParameters>
                <_Color>(0.8, 0.15, 0.3, 0)</_Color>
                <_DistortionTex>/Other/Ripples</_DistortionTex>
                <_distortionIntensity>0.05</_distortionIntensity>
                <_DistortionScale>0.4</_DistortionScale>
                <_distortionScrollSpeed>(0.07, 0.07, 0.0)</_distortionScrollSpeed>
            </shaderParameters>
        </graphicData>
        <plant>
            <fertilityMin>0.1</fertilityMin>
            <fertilitySensitivity>0.1</fertilitySensitivity>
            <growDays>1</growDays>
            <wildClusterRadius>2</wildClusterRadius>
            <wildClusterWeight>20</wildClusterWeight>
            <visualSizeRange>0.8~1.0</visualSizeRange>
            <topWindExposure>0.025</topWindExposure>
            <wildOrder>2</wildOrder>
        </plant>
        <modExtensions>
            <li Class="AlienBiomes.BiomePlantControl">
                <terrainTags>
                    <li>Water</li>
                    <li>Salty</li>
                    <li>Shallow</li>
                    <li>ABSaltyShallow</li>
                </terrainTags>
            </li>
            <li Class="AlienBiomes.Plant_Bioluminescence_ModExtension">
                <alphaMultiplier>5.575</alphaMultiplier>
			</li>
        </modExtensions>
    </ThingDef>

