<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--==================== Raw Soothing Honey ====================-->

    <ThingDef ParentName="PlantFoodRawBase">
        <defName>SZ_RawSoothingHoney</defName>
        <label>raw soothing honey</label>
        <description>Raw honey from a soothing stalk. Absolutely delicious, even raw!</description>
        <graphicData>
            <texPath>Things/Items/Resources/PlantFoodRaw/Honey/SZ_RawSoothingHoney</texPath>
        </graphicData>
        <statBases>
            <MarketValue>1.2</MarketValue>
            <Mass>0.027</Mass>
        </statBases>
        <comps>
            <li Class="CompProperties_Rottable">
                <daysToRotStart>30</daysToRotStart>
                <rotDestroys>true</rotDestroys>
            </li>
        </comps>
        <ingestible>
            <preferability>RawTasty</preferability>
            <tasteThought>SZ_AteSoothingHoney</tasteThought>
            <foodType>VegetableOrFruit</foodType>
        </ingestible>
    </ThingDef>

