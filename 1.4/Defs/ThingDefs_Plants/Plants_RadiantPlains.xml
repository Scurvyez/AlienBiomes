<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--==================== Radiant Grass Plant ====================-->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_RadiantGrass</defName>
        <label>radiant grass</label>
        <thingClass>AlienBiomes.Plant_Shimmering</thingClass>
        <description>A lush and beautiful grass found all over the radiant plains. It shimmers in the light.</description>
        <statBases>
            <MaxHitPoints>85</MaxHitPoints>
            <Flammability>1.3</Flammability>
            <Nutrition>0.09</Nutrition>
        </statBases>
        <graphicData>
            <texPath>Things/Plants/RadiantGrass</texPath>
            <graphicClass>Graphic_Random</graphicClass>
            <shaderType>TransparentPlantShimmer</shaderType>
            <shaderParameters>
                <_ShimmerSpeed>0.45</_ShimmerSpeed>
                <_NoiseTex>/Other/whiteOnlyNoise</_NoiseTex>
                <_NoiseTint>(0.25, 1, 1, 1)</_NoiseTint>
            </shaderParameters>
        </graphicData>
        <hideAtSnowDepth>0.9</hideAtSnowDepth>
        <altitudeLayer>LowPlant</altitudeLayer>
        <ingestible />
        <plant>
            <fertilityMin>0.05</fertilityMin>
            <fertilitySensitivity>0.3</fertilitySensitivity>
            <growDays>2.5</growDays>
            <leaflessGraphicPath>Things/Plants/RadiantGrass_Leafless</leaflessGraphicPath>
            <harvestWork>40</harvestWork>
            <maxMeshCount>9</maxMeshCount>
            <visualSizeRange>0.4~0.6</visualSizeRange>
            <topWindExposure>0.4</topWindExposure>
            <wildOrder>1</wildOrder>
        </plant>
        <modExtensions>
            <li Class="BiomesCore.DefModExtensions.Biomes_PlantControl">
                <terrainTags>
                    <li>Soil</li>
                    <li>Stony</li>
                    <li>Rocky</li>
                </terrainTags>
            </li>
        </modExtensions>
    </ThingDef>

