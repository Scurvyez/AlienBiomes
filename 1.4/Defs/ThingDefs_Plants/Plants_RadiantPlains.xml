    <!--==================== Cerulean Honeytree ====================-->
    <!-- This tree will produce a woody pulp which can then be manufactured into a "WoodLog" variant -->
    <!-- Can move around at night. -->
    <!-- thingClass = AlienBiomes.Plant_Mobile -->
    <!-- tickType = Long -->
    <!-- Glows at night. -->

    <ThingDef ParentName="PlantBaseNonEdible">
        <defName>SZ_CeruleanHoneytree</defName>
        <thingClass>AlienBiomes.Plant_Mobile</thingClass>
        <drawerType>MapMeshAndRealTime</drawerType>
        <label>cerulean honeytree</label>
        <description>These trees attract a plethora of small invertebrate life as inside their blueish-green exterior lies a sweet, yet immediately sickly to humanlikes, nectar with similar viscosity to terrestrial honey.</description>
        <graphicData>
            <texPath>Things/Trees/CeruleanHoneytree</texPath>
            <graphicClass>Graphic_Random</graphicClass>
            <shaderType>TransparentPlant</shaderType>
            <drawSize>1.00</drawSize>
            <shadowData>
                <volume>(0, 0, 0)</volume>
                <offset>(0, 0, 0)</offset>
            </shadowData>
        </graphicData>
        <statBases>
            <Beauty>2</Beauty>
            <BeautyOutdoors>2</BeautyOutdoors>
        </statBases>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <altitudeLayer>BuildingOnTop</altitudeLayer>
        <tickerType>Long</tickerType>
        <fillPercent>0.9</fillPercent>
        <plant>
            <fertilityMin>0.5</fertilityMin>
            <fertilitySensitivity>0.5</fertilitySensitivity>
            <growDays>40</growDays>
            <leaflessGraphicPath>Things/Trees/CeruleanHoneytree_Leafless</leaflessGraphicPath>
            <harvestWork>250</harvestWork>
            <harvestYield>5</harvestYield>
            <harvestTag>Standard</harvestTag>
            <harvestAfterGrowth>0.45</harvestAfterGrowth>
            <harvestedThingDef>SZ_CeruleanHoney</harvestedThingDef> <!-- CHANGE THIS SHIT, AGAIN... -->
            <harvestFailable>true</harvestFailable>
            <topWindExposure>0.70</topWindExposure>
            <visualSizeRange>0.10~5.00</visualSizeRange>
            <sowMinSkill>9</sowMinSkill>
            <sowTags>
                <li>Ground</li>
            </sowTags>
            <wildOrder>2</wildOrder>
            <mustBeWildToSow>false</mustBeWildToSow>
            <wildClusterRadius>12</wildClusterRadius>
            <wildClusterWeight>150</wildClusterWeight>
        </plant>
        <comps>
            <li Class="CompProperties_HeatPusher">
                <compClass>CompHeatPusher</compClass>
                <heatPerSecond>2</heatPerSecond>
                <heatPushMaxTemperature>4</heatPushMaxTemperature>
            </li>
            <li Class="AlienBiomes.CompProperties_TimedGlower">
                <overlightRadius>2.50</overlightRadius>
                <glowRadius>5.20</glowRadius>
                <glowColor>(242, 13, 13, 0)</glowColor>
                <startTime>0.75</startTime>
                <stopTime>0.20</stopTime>
            </li>
            <li Class="AlienBiomes.CompProperties_TimedHarvest">
                <harvestStartTime>0.75</harvestStartTime>
                <harvestStopTime>0.20</harvestStopTime>
                <harvestSeasons>
                    <li>Spring</li>
                    <li>Summer</li>
                </harvestSeasons>
            </li>
            <li Class="AlienBiomes.CompProperties_GraphicsOverlay">
                <graphicElements>
                    <li>
                        <texPath>Things/Trees/Overlays/CeruleanHoneytree/OverLeaf</texPath>
                        <graphicClass>Graphic_Single</graphicClass>
                        <shaderType>TransparentPlantShimmer</shaderType>
                        <shaderParameters>
                            <_ShimmerSpeed>0.85</_ShimmerSpeed>
                            <_NoiseTex>/Other/whiteOnlyNoise</_NoiseTex>
                            <_NoiseTint>(0.9, 0.3, 1, 1)</_NoiseTint>
                        </shaderParameters>
                        <drawSize>1.00</drawSize>
                        <drawOffset>(0, 0, 0.1)</drawOffset>
                        <damageData>
                            <enabled>false</enabled>
                        </damageData>
                    </li>
                    <li>
                        <texPath>Things/Trees/Overlays/CeruleanHoneytree/GlowingSpots</texPath>
                        <graphicClass>Graphic_Single</graphicClass>
                        <shaderType>TransparentPlantPulse</shaderType>
                        <shaderParameters>
                            <_PulseSpeed>1.75</_PulseSpeed>
                            <_PulseLength>1.0</_PulseLength>
                            <_PulseOrigin>(0.5, 0.5, 0, 0)</_PulseOrigin>
                        </shaderParameters>
                        <drawSize>1.00</drawSize>
                        <drawOffset>(0, 0, 0.2)</drawOffset>
                        <damageData>
                            <enabled>false</enabled>
                        </damageData>
                    </li>
                </graphicElements>
            </li>
        </comps>
        <modExtensions>
            <li Class="BiomesCore.DefModExtensions.Biomes_PlantControl">
                <terrainTags>
                    <li>Soil</li>
                </terrainTags>
            </li>
            <li Class="AlienBiomes.Plant_Mobile_ModExtension">
                <startTime>0.75</startTime>
                <stopTime>0.20</stopTime>
            </li>
        </modExtensions>
