<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!--==================== Plant Sound Effects ====================-->

	<SoundDef>
		<defName>SZ_PlantShimmering</defName>
		<context>MapOnly</context>
        <maxSimultaneous>4</maxSimultaneous>
        <priorityMode>PrioritizeNearest</priorityMode>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>PlantShimmering</clipFolderPath>
					</li>
				</grains>
				<sustainLoop>True</sustainLoop>
                <volumeRange>60~60</volumeRange>
                <pitchRange>0.95~1.05</pitchRange>
                <distRange>0~60</distRange>
			</li>
		</subSounds>
	</SoundDef>

</Defs>