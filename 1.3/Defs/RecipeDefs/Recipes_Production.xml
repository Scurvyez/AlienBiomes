<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--==================== Elder Woody Pulp ====================-->

    <RecipeDef>
        <defName>SZ_ProcessElderWoodLog</defName>
        <label>process elder wood log</label>
        <description>Process elder wood shavings for industrial uses. The resulting logs can be used for walls and weaponry.</description>
        <jobString>Processing elder tree shavings into usable wood.</jobString>
        <workAmount>400</workAmount>
        <workSkill>Crafting</workSkill>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Sculpt</effectWorking>
        <soundWorking>Recipe_Sculpt</soundWorking>
        <unfinishedThingDef>UnfinishedElderWoodLog</unfinishedThingDef>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>SZ_FragrantWoodShavings</li>
                    </thingDefs>
                </filter>
                <count>2</count>
            </li>
        </ingredients>
        <products>
            <SZ_ElderWoodLog>5</SZ_ElderWoodLog>
        </products>
        <fixedIngredientFilter>
            <thingDefs>
                <li>SZ_FragrantWoodShavings</li>
            </thingDefs>
        </fixedIngredientFilter>
    </RecipeDef>

</Defs>