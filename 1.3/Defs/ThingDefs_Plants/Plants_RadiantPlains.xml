<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--==================== Radiant Grass Plant ====================-->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_Plant_RadiantGrass</defName>
        <label>radiant grass</label>
        <description>PLACEHOLDER</description> <!-- change this -->
        <statBases>
            <MaxHitPoints>85</MaxHitPoints>
            <Flammability>1.3</Flammability>
            <Nutrition>0.50</Nutrition>
        </statBases>
        <graphicData>
            <texPath>Things/Plants/RadiantGrass</texPath>
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <hideAtSnowDepth>0.5</hideAtSnowDepth>
        <altitudeLayer>LowPlant</altitudeLayer>
        <ingestible />
        <plant>
            <fertilityMin>0.05</fertilityMin>
            <fertilitySensitivity>0.3</fertilitySensitivity>
            <growDays>2.5</growDays>
            <leaflessGraphicPath>Things/Plants/RadiantGrass_Leafless</leaflessGraphicPath>
            <harvestWork>40</harvestWork>
            <maxMeshCount>9</maxMeshCount>
            <visualSizeRange>0.4~0.6</visualSizeRange>
            <topWindExposure>0.4</topWindExposure>
            <wildOrder>1</wildOrder>
        </plant>
        <modExtensions>
            <li Class="BiomesCore.DefModExtensions.Biomes_PlantControl">
                <terrainTags>
                    <li>Soil</li>
                </terrainTags>
            </li>
        </modExtensions>
    </ThingDef>

    <!--==================== Radiant Tall Grass Plant ====================-->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_Plant_RadiantTallGrass</defName>
        <label>radiant tall grass</label>
        <description>PLACEHOLDER</description> <!-- change this -->
        <statBases>
            <MaxHitPoints>90</MaxHitPoints>
            <Flammability>1.3</Flammability>
            <Nutrition>0.50</Nutrition>
        </statBases>
        <pathCost>14</pathCost>
        <graphicData>
            <texPath>Things/Plants/RadiantGrass</texPath>
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <hideAtSnowDepth>0.5</hideAtSnowDepth>
        <altitudeLayer>LowPlant</altitudeLayer>
        <ingestible />
        <plant>
            <fertilitySensitivity>0.7</fertilitySensitivity>
            <growDays>3</growDays>
            <harvestWork>60</harvestWork>
            <maxMeshCount>9</maxMeshCount>
            <visualSizeRange>0.7~0.85</visualSizeRange>
            <topWindExposure>0.4</topWindExposure>
            <wildClusterRadius>4</wildClusterRadius>
            <wildClusterWeight>10</wildClusterWeight>
            <wildOrder>1</wildOrder>
        </plant>
        <modExtensions>
            <li Class="BiomesCore.DefModExtensions.Biomes_PlantControl">
                <terrainTags>
                    <li>Soil</li>
                </terrainTags>
            </li>
        </modExtensions>
      </ThingDef>

    <!--==================== Spiral Stalk Plant ====================-->
    <!-- only spawns on soothing sand -->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_SpiralStalk</defName> <!-- change this -->
        <label>spiral stalk</label> <!-- change this -->
        <description>PLACEHOLDER</description> <!-- change this -->
        <statBases>
            <MaxHitPoints>120</MaxHitPoints>
            <Nutrition>0.50</Nutrition>
            <DeteriorationRate>0.00</DeteriorationRate>
        </statBases>
        <graphicData>
            <texPath>Things/Plants/SpiralStalk</texPath> <!-- change this -->
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <fillPercent>0.20</fillPercent>
        <pathCost>30</pathCost>
        <plant>
            <fertilityMin>0.1</fertilityMin>
            <fertilitySensitivity>0.5</fertilitySensitivity>
            <growDays>16</growDays>
            <leaflessGraphicPath>Things/Plants/SpiralStalk_Leafless</leaflessGraphicPath> <!-- change this -->
            <immatureGraphicPath>Things/Plants/SpiralStalk_Immature</immatureGraphicPath> <!-- change this -->
            <harvestYield>10</harvestYield>
            <harvestWork>250</harvestWork>
            <harvestTag>Standard</harvestTag>
            <harvestAfterGrowth>0.30</harvestAfterGrowth>
            <harvestedThingDef>RawBerries</harvestedThingDef> <!-- change this -->
            <wildClusterRadius>5</wildClusterRadius>
            <wildClusterWeight>250</wildClusterWeight>
            <visualSizeRange>0.95~1.7</visualSizeRange>
            <topWindExposure>0.3</topWindExposure>
            <wildOrder>2</wildOrder>
            <purpose>Food</purpose>
        </plant>
        <comps>
            <li Class="AlienBiomes.CompProperties_TimedGlower">
                <overlightRadius>2.25</overlightRadius>
                <glowRadius>3.25</glowRadius>
                <glowColor>(250, 180, 188, 0)</glowColor>
                <startTime>0.80</startTime>
                <stopTime>0.20</stopTime>
            </li>
        </comps>
        <modExtensions>
            <li Class="BiomesCore.DefModExtensions.Biomes_PlantControl">
                <terrainTags>
                    <li>Sandy</li>
                </terrainTags>
            </li>
        </modExtensions>
    </ThingDef>

