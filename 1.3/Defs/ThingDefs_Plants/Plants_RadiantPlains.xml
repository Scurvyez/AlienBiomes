    <!--==================== Soothing Stalk Plant ====================-->
    <!-- Only spawns on soothing sand. -->
    <!-- Produces raw soothing honey. -->
    <!-- Harvestable only at night. -->
    <!-- Glows at night. -->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_SoothingStalk</defName>
        <label>soothing stalk</label>
        <description>A vibrant plant only found on the pink sands of radiant plains. They can be farmed for their sweet, pinkish honey. They seemingly spring to life at sundown.</description>
        <statBases>
            <MaxHitPoints>120</MaxHitPoints>
            <Nutrition>0.25</Nutrition>
            <DeteriorationRate>0.00</DeteriorationRate>
        </statBases>
        <graphicData>
            <texPath>Things/Plants/SoothingStalk</texPath>
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <altitudeLayer>BuildingOnTop</altitudeLayer>
        <tickerType>Long</tickerType>
        <fillPercent>0.20</fillPercent>
        <pathCost>30</pathCost>
        <plant>
            <fertilityMin>0.1</fertilityMin>
            <fertilitySensitivity>0.5</fertilitySensitivity>
            <growDays>16</growDays>
            <leaflessGraphicPath>Things/Plants/SoothingStalk_Leafless</leaflessGraphicPath>
            <immatureGraphicPath>Things/Plants/SoothingStalk_Immature</immatureGraphicPath>
            <harvestYield>10</harvestYield>
            <harvestWork>250</harvestWork>
            <harvestTag>Standard</harvestTag>
            <harvestAfterGrowth>0.30</harvestAfterGrowth>
            <harvestedThingDef>SZ_RawSoothingHoney</harvestedThingDef>
            <wildClusterRadius>5</wildClusterRadius>
            <wildClusterWeight>250</wildClusterWeight>
            <visualSizeRange>0.95~1.7</visualSizeRange>
            <topWindExposure>0.3</topWindExposure>
            <wildOrder>2</wildOrder>
            <purpose>Food</purpose>
        </plant>
        <comps>
            <li Class="AlienBiomes.CompProperties_TimedGlower">
                <overlightRadius>2.25</overlightRadius>
                <glowRadius>3.25</glowRadius>
                <glowColor>(250, 180, 188, 0)</glowColor>
                <startTime>0.80</startTime>
                <stopTime>0.20</stopTime>
            </li>
            <li Class="AlienBiomes.CompProperties_TimedHarvest">
                <harvestStartTime>0.80</harvestStartTime>
                <harvestStopTime>0.20</harvestStopTime>
                <harvestSeasons>
                    <li>Spring</li>
                    <li>Summer</li>
                </harvestSeasons>
            </li>
        </comps>
        <modExtensions>
            <li Class="BiomesCore.DefModExtensions.Biomes_PlantControl">
                <terrainTags>
                    <li>Sandy</li>
                </terrainTags>
            </li>
        </modExtensions>
    </ThingDef>

    <!--==================== Copacetic Cone Plant ====================-->
    <!-- Only spawns on radiant soil and rich radiant soil. -->
    <!-- Emits a hediff to sooth nearby pawns. -->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_CopaceticCone</defName>
        <label>copacetic cone</label>
        <description>Curious-looking plants that resemble urb-world soft serve ice cream. They release an exquisite honey!</description>
        <statBases>
            <Nutrition>0.20</Nutrition>
            <MaxHitPoints>85</MaxHitPoints>
            <Flammability>1.30</Flammability>
            <DeteriorationRate>0.00</DeteriorationRate>
        </statBases>
        <graphicData>
            <texPath>Things/Plants/CopaceticCone</texPath>
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <altitudeLayer>BuildingOnTop</altitudeLayer>
        <tickerType>Long</tickerType>
        <fillPercent>0.80</fillPercent>
        <pathCost>6</pathCost>
        <ingestible />
        <plant>
            <fertilityMin>0.5</fertilityMin>
            <fertilitySensitivity>0.5</fertilitySensitivity>
            <growDays>14</growDays>
            <leaflessGraphicPath>Things/Plants/CopaceticCone_Leafless</leaflessGraphicPath>
            <immatureGraphicPath>Things/Plants/CopaceticCone_Immature</immatureGraphicPath>
            <harvestYield>10</harvestYield>
            <harvestWork>250</harvestWork>
            <harvestTag>Standard</harvestTag>
            <harvestAfterGrowth>0.30</harvestAfterGrowth>
            <harvestedThingDef>SZ_RawCopaceticHoney</harvestedThingDef>
            <wildClusterRadius>5</wildClusterRadius>
            <wildClusterWeight>250</wildClusterWeight>
            <visualSizeRange>0.85~2.85</visualSizeRange>
            <topWindExposure>0.3</topWindExposure>
            <wildOrder>2</wildOrder>
            <purpose>Food</purpose>
        </plant>
        <comps>
            <li Class="AlienBiomes.CompProperties_PlantFleckEmitter">
                <fleck>Twinkle</fleck>
                <burstCount>1</burstCount>
                <offset>(0.0, 0.0, 3.00)</offset>
                <scale>0.25~0.45</scale>
                <colorA>(56, 163, 169)</colorA>
                <colorB>(124, 251, 208)</colorB>
                <rotationRate>-15~15</rotationRate>
                <soundOnEmission>SZ_PlantShimmering</soundOnEmission>
                <velocityX>0.3~0.7</velocityX>
                <velocityY>0.0~5.75</velocityY>
            </li>
            <li Class="AlienBiomes.CompProperties_EffectorWithRadius">
                <releaseRadius>3</releaseRadius>
                <fleckReleased>PsycastAreaEffect</fleckReleased>
                <appliedHediff MayRequire="Ludeon.RimWorld.Iedology">WorkDrive</appliedHediff>
                <tickInterval>15000</tickInterval>
                <radiusOutlineColor>(102, 255, 204)</radiusOutlineColor>
            </li>
        </comps>
        <modExtensions>
            <li Class="BiomesCore.DefModExtensions.Biomes_PlantControl">
                <terrainTags>
                    <li>Soil</li>
                </terrainTags>
            </li>
        </modExtensions>
    </ThingDef>

