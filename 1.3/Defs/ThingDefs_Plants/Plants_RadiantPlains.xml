    <!--==================== Elder Blushing Tree ====================-->
    <!-- This tree will produce a woody pulp which can then be manufactured into a "WoodLog" variant -->
    <!-- Glows at night. -->

    <ThingDef ParentName="AlienDeciduousTreeBase" Name="SZ_ElderBlushingTree">
        <defName>SZ_ElderBlushingTree</defName>
        <label>elder blushing tree</label>
        <description>These trees attract clawed predators who love the fragrant bark beneath their razor-like claws. Generations of animals scratching at the surface and subsequent evolutionary softening of the trees' woody trunks has led to the loss of natural woody bi-products.\n\nInstead, these trees produce a pulp-like substance that can be harvested and refined into wood planks.</description>
        <graphicData>
            <texPath>Things/Trees/ElderBlushingTree</texPath>
            <graphicClass>Graphic_Random</graphicClass>
            <shadowData>
                <volume>(0.2, 0.35, 0.13)</volume>
                <offset>(-0.03,0,-0.44)</offset>
            </shadowData>
        </graphicData>
        <statBases>
            <Beauty>2</Beauty>
            <BeautyOutdoors>2</BeautyOutdoors>
            <Nutrition>0.01</Nutrition>
        </statBases>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <altitudeLayer>BuildingOnTop</altitudeLayer>
        <tickerType>Long</tickerType>
        <fillPercent>0.90</fillPercent>
        <plant>
            <fertilityMin>0.5</fertilityMin>
            <fertilitySensitivity>0.5</fertilitySensitivity>
            <growDays>40</growDays>
            <leaflessGraphicPath>Things/Trees/ElderBlushingTree_Leafless</leaflessGraphicPath>
            <harvestWork>250</harvestWork>
            <harvestYield>2</harvestYield>
            <harvestTag>Standard</harvestTag>
            <harvestAfterGrowth>0.45</harvestAfterGrowth>
            <harvestedThingDef>SZ_FragrantWoodShavings</harvestedThingDef>
            <harvestFailable>true</harvestFailable>
            <topWindExposure>0.70</topWindExposure>
            <visualSizeRange>1.00~5.15</visualSizeRange>
            <sowMinSkill>7</sowMinSkill>
            <sowTags>
                <li>Ground</li>
            </sowTags>
            <wildOrder>2</wildOrder>
            <mustBeWildToSow>false</mustBeWildToSow>
            <wildClusterRadius>22</wildClusterRadius>
            <wildClusterWeight>250</wildClusterWeight>
        </plant>
        <comps>
            <li Class="AlienBiomes.CompProperties_TimedGlower">
                <overlightRadius>2.50</overlightRadius>
                <glowRadius>3.20</glowRadius>
                <glowColor>(63, 216, 188, 0)</glowColor>
                <startTime>0.80</startTime>
                <stopTime>0.15</stopTime>
            </li>
        </comps>
        <modExtensions>
            <li Class="BiomesCore.DefModExtensions.Biomes_PlantControl">
                <terrainTags>
                    <li>Soil</li>
                </terrainTags>
            </li>
        </modExtensions>
    </ThingDef>

