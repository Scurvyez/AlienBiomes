<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--==================== Enlightened Grass Plant ====================-->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_Plant_EnlightenedGrass</defName>
        <label>enlightened grass</label>
        <description>PLACEHOLDER</description>
        <statBases>
            <MaxHitPoints>85</MaxHitPoints>
            <Flammability>1.3</Flammability>
            <Nutrition>0.50</Nutrition>
        </statBases>
        <graphicData>
            <texPath>Things/Plant/EnlightenedGrassPlant</texPath>
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <hideAtSnowDepth>0.5</hideAtSnowDepth>
        <ingestible />
        <plant>
            <fertilityMin>0.05</fertilityMin>
            <fertilitySensitivity>0.3</fertilitySensitivity>
            <growDays>2.5</growDays>
            <leaflessGraphicPath>Things/Plant/EnlightenedGrassPlant_Leafless</leaflessGraphicPath>
            <harvestWork>40</harvestWork>
            <maxMeshCount>9</maxMeshCount>
            <visualSizeRange>0.4~0.6</visualSizeRange>
            <topWindExposure>0.4</topWindExposure>
            <wildOrder>1</wildOrder>
        </plant>
    </ThingDef>

    <!--==================== Enlightened Tall Grass Plant ====================-->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_Plant_EnlightenedTallGrass</defName>
        <label>enlightened tall grass</label>
        <description>PLACEHOLDER</description>
        <statBases>
            <MaxHitPoints>90</MaxHitPoints>
            <Flammability>1.3</Flammability>
            <Nutrition>0.50</Nutrition>
        </statBases>
        <pathCost>14</pathCost>
        <graphicData>
            <texPath>Things/Plant/EnlightenedGrassPlant</texPath>
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <hideAtSnowDepth>0.5</hideAtSnowDepth>
        <ingestible />
        <plant>
            <fertilitySensitivity>0.7</fertilitySensitivity>
            <growDays>3</growDays>
            <harvestWork>60</harvestWork>
            <maxMeshCount>9</maxMeshCount>
            <visualSizeRange>0.7~0.85</visualSizeRange>
            <topWindExposure>0.4</topWindExposure>
            <wildClusterRadius>4</wildClusterRadius>
            <wildClusterWeight>10</wildClusterWeight>
            <wildOrder>1</wildOrder>
        </plant>
      </ThingDef>

    <!--==================== Spiral Stalk Plant Base (A) ====================-->

    <ThingDef ParentName="PlantBase" Name="SpiralStalkBase">
        <defName>SZ_Plant_SpiralStalkA</defName>
        <label>spiral stalk</label>
        <description>PLACEHOLDER</description>
        <thingClass>Alien_Biomes.Plant_NightBlooming</thingClass>
        <statBases>
            <MaxHitPoints>120</MaxHitPoints>
            <Nutrition>0.50</Nutrition>
        </statBases>
        <graphicData>
            <texPath>Things/Plant/SpiralStalkPlant/SpiralStalkPlantA</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <fillPercent>0.20</fillPercent>
        <pathCost>30</pathCost>
        <plant>
            <fertilityMin>0.5</fertilityMin>
            <fertilitySensitivity>0.5</fertilitySensitivity>
            <growDays>6</growDays>
            <leaflessGraphicPath>Things/Plant/SpiralStalkPlant_Leafless/SpiralStalkPlant_LeaflessA</leaflessGraphicPath>
            <immatureGraphicPath>Things/Plant/SpiralStalkPlant_Immature/SpiralStalkPlant_ImmatureA</immatureGraphicPath>
            <harvestYield>10</harvestYield>
            <harvestWork>250</harvestWork>
            <harvestTag>Standard</harvestTag>
            <harvestAfterGrowth>0.30</harvestAfterGrowth>
            <harvestedThingDef>RawBerries</harvestedThingDef>
            <wildClusterRadius>5</wildClusterRadius>
            <wildClusterWeight>250</wildClusterWeight>
            <visualSizeRange>0.95~1.7</visualSizeRange>
            <topWindExposure>0.3</topWindExposure>
            <wildOrder>2</wildOrder>
            <purpose>Food</purpose>
        </plant>
        <modExtensions>
            <li Class="Alien_Biomes.NightBlooming_ModExtension">
              <nightfallGraphicPath>Things/Plant/SpiralStalkPlant_NightFall/SpiralStalkPlantA</nightfallGraphicPath>
            </li>
        </modExtensions>
    </ThingDef>

