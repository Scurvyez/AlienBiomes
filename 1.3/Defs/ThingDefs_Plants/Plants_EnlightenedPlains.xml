    <!--==================== TEST TREE ====================-->

    <!-- This tree will produce a woody pulp which can then be manufactured into "WoodLog" -->

    <ThingDef ParentName="PlantBase">
        <defName>SZ_TEST_TREE</defName>
        <label>PLACEHOLDER</label>
        <description>PLACEHOLDER</description>
        <graphicData>
            <texPath>Things/Trees/TEST_TREE</texPath>
            <graphicClass>Graphic_Random</graphicClass>
            <shadowData>
                <volume>(0.2, 0.35, 0.13)</volume>
                <offset>(-0.03,0,-0.44)</offset>
            </shadowData>
        </graphicData>
        <statBases>
            <Beauty>2</Beauty>
            <BeautyOutdoors>2</BeautyOutdoors>
            <Nutrition>0.15</Nutrition>
        </statBases>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <fillPercent>0.90</fillPercent>
        <plant>
            <fertilityMin>0.5</fertilityMin>
            <fertilitySensitivity>0.5</fertilitySensitivity>
            <growDays>40</growDays>
            <leaflessGraphicPath>Things/Plants/TreeOak_Leafless</leaflessGraphicPath>
            <harvestWork>1111</harvestWork>
            <harvestYield>35</harvestYield>
            <harvestTag>Standard</harvestTag>
            <harvestedThingDef>WoodLog</harvestedThingDef>
            <harvestFailable>true</harvestFailable>
            <topWindExposure>0.70</topWindExposure>
            <visualSizeRange>0.95~3.5</visualSizeRange>
            <sowMinSkill>7</sowMinSkill>
            <sowTags>
                <li>Ground</li>
            </sowTags>
            <wildOrder>2</wildOrder>
            <mustBeWildToSow>false</mustBeWildToSow>
            <wildClusterRadius>8</wildClusterRadius>
            <wildClusterWeight>250</wildClusterWeight>
        </plant>
    </ThingDef>

    <!--==================== Copacetic Cone Plant (Base) ====================-->

    <ThingDef ParentName="PlantBase" Name="CopaceticConeBase">
        <defName>SZ_CopaceticConeBase</defName>
        <label>copacetic cone</label>
        <description>PLACEHOLDER</description>
        <statBases>
            <Nutrition>0.15</Nutrition>
            <MaxHitPoints>85</MaxHitPoints>
            <Flammability>1.30</Flammability>
            <DeteriorationRate>0.00</DeteriorationRate>
        </statBases>
        <graphicData>
            <texPath>Things/Plants/CopaceticCone</texPath>
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <fillPercent>0.20</fillPercent>
        <pathCost>30</pathCost>
        <ingestible />
        <plant>
            <fertilityMin>0.5</fertilityMin>
            <fertilitySensitivity>0.5</fertilitySensitivity>
            <growDays>14</growDays>
            <leaflessGraphicPath>Things/Plants/CopaceticCone_Leafless</leaflessGraphicPath>
            <immatureGraphicPath>Things/Plants/CopaceticCone_Immature</immatureGraphicPath>
            <wildClusterRadius>5</wildClusterRadius>
            <wildClusterWeight>250</wildClusterWeight>
            <visualSizeRange>0.85~2.85</visualSizeRange>
            <topWindExposure>0.3</topWindExposure>
            <wildOrder>2</wildOrder>
        </plant>
        <comps>
            <li Class="AlienBiomes.CompProperties_TimedGlower">
                <glowRadius>1.75</glowRadius>
                <glowColor>(63,216,188,0)</glowColor>
                <startTime>0.75</startTime>
                <stopTime>0.20</stopTime>
            </li>
        </comps>
    </ThingDef>

